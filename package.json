{
  "name": "andrewensley-com",
  "version": "2.0.0",
  "description": "AndrewEnsley.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/aensley/andrewensley-com"
  },
  "author": "aensley@users.noreply.github.com",
  "license": "UNLICENSED",
  "private": true,
  "standard": {
    "globals": [
      "Waypoint"
    ],
    "ignore": [
      "/src/assets/js/fontawesome.js"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "test"
  ],
  "scripts": {
    "dev": "next",
    "build": "npm run copy:functions && npm run favicon && next build && next export",
    "favicon": "real-favicon generate src/faviconDescription.json src/faviconData.json public",
    "copy:functions": "cp -af src/functions/ functions/",
    "start": "next start",
    "typecheck": "tsc",
    "precommit-msg": "echo Running pre-commit checks...",
    "update": "npx --yes browserslist@latest --update-db && npx --yes npm-check-updates -u && npm update",
    "test": "npm run typecheck && standard src/assets/js/*.js && prettier --check ."
  },
  "dependencies": {
    "@cfworker/sentry": "^1.12.4",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "gray-matter": "4.0.3",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "remark": "14.0.2",
    "remark-html": "15.0.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "autoprefixer": "10.4.13",
    "cli-real-favicon": "^0.0.8",
    "eslint": "^8.29.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "postcss": "8.4.19",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.1",
    "tailwindcss": "3.2.4"
  }
}
